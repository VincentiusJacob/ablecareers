{
  "version": 2,
  "builds": [
    {
      "src": "client/ablecareers/package.json",
      "use": "@vercel/vite"
    },
    {
      "src": "server/package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "client/ablecareers/dist/$1"
    }
  ]
}
